{
  "name": "desarrollo_aplicaciones_front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "200 KB"
    }
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "prepare": "husky",
    "size": "size-limit",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "react-select": "^5.10.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@size-limit/file": "^11.2.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "size-limit": "^11.2.0",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
